@page "/whoami"
@using PortfolioWebsite.BlazorUI.Models.WhoAmI
@inject IArticleStore _articleStore;

<LoadablePage Page="page" /> 

@code {
    private PageInfoModel page;

    protected async override Task OnInitializedAsync()
    {
        var whoAmI = await this._articleStore.GetWhoAmIAsync();
		this.page = whoAmI.Page;
    }
}
